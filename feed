import tensorflow as tf
input1 =tf.placeholder(tf.float32)#define the format input data
input2 =tf.placeholder(tf.float32)#define the format input data
output =tf.mul(input1,input2)

with tf.Session() as sess:
  print(sess.run(output,feed_dict={input1:[7.],input2:[2.]}))#define extra data into tf. feed =喂给
